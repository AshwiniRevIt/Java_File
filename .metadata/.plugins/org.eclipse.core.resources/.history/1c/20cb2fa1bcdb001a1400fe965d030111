package testcases;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.annotations.Test;

import objectrepositories.RedifLoginPage;

public class LoginApplication {
	
	@Test
	public void Login() {
		
		System.setProperty("webdriver.chrome.driver","C://Users//Public//Downloads//chromedriver_win32//chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("https://mail.rediff.com/cgi-bin/login.cgi");
		
		RedifLoginPage rd = new RedifLoginPage(driver);
		rd.Email().sendKeys("ashwini");
		rd.Password().sendKeys("Ash123");
		
		
		
		
	}

}
